#include <iostream>
#include <stdio.h>

using namespace std;

template <typename T> class Stack {
public:
	Stack();
	~Stack();

	class Node;
	Node *start;

	bool Push(T &t);
	T Pop();
	bool IsEmpty();

	class Node {
	public:
		T *data;
		Node *next;
	};

};

template <typename T> Stack<T>::Stack() {
	start = NULL;
}

template <typename T> Stack<T>::~Stack() {
	delete(start);
}

