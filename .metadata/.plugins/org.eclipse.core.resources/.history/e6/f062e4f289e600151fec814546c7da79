#include <iostream>
#include <stdio.h>

using namespace std;

template <typename T> class Stack {
public:
	Stack();
	~Stack();

	T data;
	Stack *next;

	void Push(T &t);
	T Pop();
	bool Empty();
	T Top();

};

template <typename T> Stack<T>::Stack() {
	data = NULL;
	next = NULL;
}

template <typename T> Stack<T>::~Stack() {
	delete(data);
}

template <typename T> bool Stack<T>::Push( T &ref) {

	Stack *stack = new Stack;
	stack->data = ref;
	this->next = stack;
}


int main() {

	return 0;
}
