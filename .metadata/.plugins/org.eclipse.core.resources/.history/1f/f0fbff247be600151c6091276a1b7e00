#include <iostream>
#include <stdio.h>

using namespace std;

template <typename T> class Stack {
public:
	Stack();
	~Stack();

	Stack *data;
	Stack *next;

	bool Push(T &t);
	T Pop();
	bool Empty();
	T Top();

};

template <typename T> Stack<T>::Stack() {
	start = NULL;
}

template <typename T> Stack<T>::~Stack() {
	delete(start);
}

template <typename T> bool Stack<T>::Push( T &ref) {

	T t = ref;

}


